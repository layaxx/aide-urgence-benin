  "license": "ISC",
  "scripts": {
    "predev": "yarn generateCMSConfig",
    "dev": "next",
    "build": "next build",
    "postbuild": "yarn generateCMSConfig",
    "start": "next start",
    "export": "npm run build && next export",
    "translate": "i18next 'pages/**/*.{js,jsx,ts,tsx}' 'components/**/*.{js,jsx,ts,tsx}'",
    "posttranslate": "yarn generateCMSConfig",
    "cms": "npx netlify-cms-proxy-server",
    "generateCMSConfig": "node ./scripts/generateCMSConfig.mjs",
    "lint": "next lint",
    "prepare": "husky install",
    "pre-commit": "yarn translate && yarn lint && git add -A ."
  },
  "dependencies": {
    "@fontsource/spectral": "^4.5.0",
    "@picocss/pico": "^1.4.2",
    "dayjs": "^1.10.7",
    "i18next-parser": "^5.3.0",
    "markdown-to-jsx": "^7.1.5",
    "netlify-cms-oauth-provider-node": "^2.0.0",
    "next": "latest",
    "next-i18next": "^10.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-share": "^4.4.0",
    "react-social-icons": "^5.6.1",
    "sharp": "^0.29.3",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "frontmatter-markdown-loader": "^2.0.0",
    "husky": "^7.0.4",
    "netlify-cms-proxy-server": "^1.3.22",
    "typescript": "^4.5.4"
  }
}
