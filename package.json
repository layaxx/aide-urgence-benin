    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "frontmatter-markdown-loader": "^2.0.0",
    "husky": "^7.0.4",
    "netlify-cms-proxy-server": "^1.3.22",
    "typescript": "^4.5.4",
    "yarn-audit-fix": "^9.0.10"
