{
  "name": "aide-urgence-benin",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:layaxx/aide-urgence-benin.git",
  "author": "Yannick Lang <layaxx@gmx.de>",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "clean": "rimraf dist src/_js",
    "build": "npm-run-all clean build:ts build:11ty",
    "dev": "npm-run-all clean build:ts -p watch:* -- --watch",
    "build:11ty": "eleventy --quiet",
    "watch:11ty": "eleventy --serve --quiet",
    "build:ts": "tsc --outDir src/_js",
    "watch:ts": "yarn build:ts --watch"
  }
}
